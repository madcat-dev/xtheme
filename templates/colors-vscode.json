        "editorGroupHeader.tabsBorder": "${background}",
        "tab.activeBackground": "${background}",
        "tab.activeForeground": "${foreground}",
        "tab.border": "${background}",
        "tab.activeBorder": "$color1",
        "tab.activeBorderTop": null,
        "tab.unfocusedActiveBorder": "${foreground}99",
        "tab.unfocusedActiveBorderTop": null,
        "tab.inactiveBackground": "${background}",
        "tab.inactiveForeground": "${foreground}99",
        "tab.unfocusedActiveForeground": "${foreground}99",
        "tab.unfocusedInactiveForeground": "${foreground}99",

        "editor.background": "${background}",
        "editor.foreground": "${foreground}",
        "editorLineNumber.foreground": "${color8}92",
        "editorLineNumber.activeForeground": "$color8",
        "editorCursor.foreground": "$color1",

        "editor.selectionBackground": "${color8}77",
        "editor.inactiveSelectionBackground": "${color8}44",
        "editor.selectionHighlightBackground": "${color8}44",
        "editor.selectionHighlightBorder": "$color8",

        "editor.wordHighlightBackground": "${color8}44",
        "editor.wordHighlightStrongBackground": "${color2}77",

        "editor.findMatchBackground": "${color1}0e",
        "editor.findMatchBorder": "$color1",
        "editor.findMatchHighlightBackground": "${color1}0e",
        "editor.findMatchHighlightBorder": "${color1}66",
        "editor.findRangeHighlightBackground": "${color8}44",
        "editor.findRangeHighlightBorder": "${background}00",

        "editor.lineHighlightBackground": "${foreground}22",
        "editorLink.activeForeground": "$color1",
        "editor.rangeHighlightBackground": "${color8}33",
        "editorWhitespace.foreground": "${color8}66",
        "editorIndentGuide.background": "${color8}44",
        "editorIndentGuide.activeBackground": "${color8}77",
        "editorRuler.foreground": "${color8}44",
        "editorCodeLens.foreground": "${color8}b0",
        "editorBracketMatch.background": "${color8}33",
        "editorBracketMatch.border": "${color8}55",

        "editorBracketHighlight.foreground1": "$color6",
        "editorBracketHighlight.foreground2": "$color5",
        "editorBracketHighlight.foreground3": "$color4",
        "editorBracketHighlight.foreground4": "$color3",
        "editorBracketHighlight.foreground5": "$color2",
        "editorBracketHighlight.foreground6": "$color1",

        "editorOverviewRuler.border": "${color8}33",
        "editorOverviewRuler.modifiedForeground": "${color3}bb",
        "editorOverviewRuler.addedForeground": "${color2}bb",
        "editorOverviewRuler.deletedForeground": "${color4}bb",
        "editorOverviewRuler.errorForeground": "$color4",
        "editorOverviewRuler.warningForeground": "$color1",

        "editorError.foreground": "$color4",
        "editorWarning.foreground": "$color1",

        "editorGutter.modifiedBackground": "${color3}bb",
        "editorGutter.addedBackground": "${color2}bb",
        "editorGutter.deletedBackground": "${color4}bb",

        "diffEditor.insertedTextBackground": "${color10}33",
        "diffEditor.removedTextBackground": "${color3}33",

        "editorWidget.background": "$(rgb_value ${background} +20)",
        "editorSuggestWidget.background": "$(rgb_value ${background} +20)",
        "editorSuggestWidget.border": "${color8}22",
        "editorSuggestWidget.highlightForeground": "$color1",
        "editorSuggestWidget.selectedBackground": "${color8}33",
        "editorHoverWidget.background": "$(rgb_value ${background} +20)",
        "editorHoverWidget.border": "${color8}22",

        "debugExceptionWidget.border": "${color8}33",
        "debugExceptionWidget.background": "$(rgb_value ${background} +20)",

        "editorMarkerNavigation.background": "$(rgb_value ${background} +20)",

        "peekView.border": "${color8}33",
        "peekViewEditor.background": "$(rgb_value ${background} +20)",
        "peekViewEditor.matchHighlightBackground": "${color1}44",
        "peekViewResult.background": "$(rgb_value ${background} +20)",
        "peekViewResult.fileForeground": "${foreground}99",
        "peekViewResult.matchHighlightBackground": "${color1}44",
        "peekViewTitle.background": "$(rgb_value ${background} +20)",
        "peekViewTitleDescription.foreground": "${foreground}99",
        "peekViewTitleLabel.foreground": "${foreground}99",

        "panel.background": "${background}",
        "panel.border": "${color8}33",
        "panelTitle.activeBorder": "$color1",
        "panelTitle.activeForeground": "${foreground}",
        "panelTitle.inactiveForeground": "${foreground}99",

        "statusBar.background": "${background}",
        "statusBar.foreground": "${foreground}",
        "statusBar.border": "${background}",
        "statusBar.debuggingBackground": "$color3",
        "statusBar.debuggingForeground": "${background}dd",
        "statusBar.noFolderBackground": "$(rgb_value ${background} +20)",
        "statusBarItem.activeBackground": "#00000050",
        "statusBarItem.hoverBackground": "#00000030",
        "statusBarItem.prominentBackground": "${color8}33",
        "statusBarItem.prominentHoverBackground": "#00000030",

        "titleBar.activeBackground": "${background}",
        "titleBar.activeForeground": "${foreground}",
        "titleBar.inactiveBackground": "${background}",
        "titleBar.inactiveForeground": "${foreground}",
        "titleBar.border": "${background}",

        "extensionButton.prominentForeground": "${background}",
        "extensionButton.prominentBackground": "$color1",
        "extensionButton.prominentHoverBackground": "${color1}b3",

        "pickerGroup.border": "${color8}33",
        "pickerGroup.foreground": "${foreground}b3",

        "debugTokenExpression.value": "${foreground}b3",
        "debugToolBar.background": "${background}",

        "walkThrough.embeddedEditorBackground": "$(rgb_value ${background} +20)",

        "gitDecoration.modifiedResourceForeground": "${color3}cc",
        "gitDecoration.deletedResourceForeground": "${color4}cc",
        "gitDecoration.untrackedResourceForeground": "${color2}cc",
        "gitDecoration.ignoredResourceForeground": "${foreground}66",
        "gitDecoration.submoduleResourceForeground": "${color13}b0",

        "settings.headerForeground": "${foreground}",
        "settings.modifiedItemIndicator": "$color2",

        "terminal.background": "${background}",
        "terminal.foreground": "${foreground}",
        "terminal.ansiBlack": "${background}",

        "terminal.ansiRed": "$color1",
        "terminal.ansiGreen": "$color2",
        "terminal.ansiYellow": "$color3",
        "terminal.ansiBlue": "$color4",
        "terminal.ansiMagenta": "$color5",
        "terminal.ansiCyan": "$color6",
        "terminal.ansiWhite": "${foreground}",
        "terminal.ansiBrightBlack": "$color8",
        "terminal.ansiBrightRed": "$color9",
        "terminal.ansiBrightGreen": "$color10",
        "terminal.ansiBrightYellow": "$color11",
        "terminal.ansiBrightBlue": "$color12",
        "terminal.ansiBrightMagenta": "$color13",
        "terminal.ansiBrightCyan": "$color14",
        "terminal.ansiBrightWhite": "$color15"
    },
    "tokenColors": [
        {
            "settings": {
                "background": "${background}",
                "foreground": "${foreground}"
            }
        },
        {
            "name": "Comment",
            "scope": ["comment"],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#5f8787"
            }
        },

        {
            "name": "String",
            "scope": ["string", "constant.other.symbol"],
            "settings": {
                "foreground": "$color2"
            }
        },

        {
            "name": "Regular Expressions and Escape Characters",
            "scope": ["string.regexp", "constant.character", "constant.other"],
            "settings": {
                "refill": "",
                "foreground": "$color6"
            }
        },

        {
            "name": "Number",
            "scope": ["constant.numeric"],
            "settings": {
                "foreground": "$color6"
            }
        },
        {
            "name": "Built-in constants",
            "scope": ["constant.language"],
            "settings": {
                "foreground": "$color6"
            }
        },

        {
            "name": "Variable",
            "scope": ["variable"],
            "settings": {
                "foreground": "$color9"
            }
        },
        {
            "name": "Member Variable",
            "scope": ["variable.member"],
            "settings": {
                "foreground": "$color1"
            }
        },
        {
            "name": "Language variable",
            "scope": ["variable.language"],
            "settings": {
                "fontStyle": "italic",
                "foreground": "$color2"
            }
        },

        {
            "name": "Storage",
            "scope": ["storage"],
            "settings": {
                "foreground": "$color14"
            }
        },
        {
            "name": "type",
            "scope": ["storage.modifier"],
            "settings": {
                "foreground": "$color11"
            }
        },
        {
            "name": "type",
            "scope": ["meta.specification.type.fortran"],
            "settings": {
                "foreground": "$color15"
            }
        },
        {
            "name": "Keyword",
            "scope": ["keyword"],
            "settings": {
                "foreground": "$color1"
            }
        },

        {
            "name": "Operators",
            "scope": ["keyword.operator", "keyword.other", "keyword.control", "keyword.logical"],
            "settings": {
                "foreground": "$color11"
            }
        },

        {
            "name": "Separators like ; or ,",
            "scope": ["punctuation.separator", "punctuation.terminator"],
            "settings": {
                "foreground": "${foreground}b0"
            }
        },
        {
            "name": "Punctuation",
            "scope": ["punctuation.section"],
            "settings": {
                "foreground": "${foreground}"
            }
        },
        {
            "name": "Accessor",
            "scope": ["punctuation.accessor"],
            "settings": {
                "foreground": "$color3"
            }
        },

        {
            "name": "Types fixes",
            "scope": [
                "source.java storage.type",
                "source.haskell storage.type",
                "source.c storage.type"
            ],
            "settings": {
                "foreground": "$color12"
            }
        },
        {
            "name": "Inherited class type",
            "scope": ["entity.other.inherited-class"],
            "settings": {
                "foreground": "$color2"
            }
        },
        {
            "name": "Lambda arrow",
            "scope": ["storage.type.function"],
            "settings": {
                "foreground": "$color14"
            }
        },
        {
            "name": "Java primitive variable types",
            "scope": ["source.java storage.type.primitive"],
            "settings": {
                "foreground": "$color2"
            }
        },

        {
            "name": "Function name",
            "scope": ["entity.name.function"],
            "settings": {
                "foreground": "$color9"
            }
        },
        {
            "name": "Function arguments",
            "scope": ["variable.parameter"],
            "settings": {
                "foreground": "$color15"
            }
        },
        {
            "name": "Function arguments",
            "scope": ["meta.parameter"],
            "settings": {
                "foreground": "$color15"
            }
        },
        {
            "name": "Function call",
            "scope": [
                "variable.function",
                "variable.annotation",
                "meta.function-call.generic",
                "support.function.go"
            ],
            "settings": {
                "foreground": "$color11"
            }
        },
        {
            "name": "Library function",
            "scope": ["support.function", "support.macro"],
            "settings": {
                "foreground": "$color1"
            }
        },

        {
            "name": "Imports and packages",
            "scope": ["entity.name.import", "entity.name.package"],
            "settings": {
                "foreground": "$color10"
            }
        },
        {
            "name": "Entity name",
            "scope": ["entity.name"],
            "settings": {
                "foreground": "$color1"
            }
        },

        {
            "name": "Tag",
            "scope": ["entity.name.tag", "meta.tag.sgml"],
            "settings": {
                "foreground": "$color2"
            }
        },
        {
            "name": "Tag start/end",
            "scope": [
                "punctuation.definition.tag.end",
                "punctuation.definition.tag.begin",
                "punctuation.definition.tag"
            ],
            "settings": {
                "foreground": "${color2}77"
            }
        },
        {
            "name": "Tag attribute",
            "scope": ["entity.other.attribute-name"],
            "settings": {
                "foreground": "$color11"
            }
        },


        {
            "name": "Library constant",
            "scope": ["support.constant"],
            "settings": {
                "fontStyle": "italic",
                "foreground": "$color3"
            }
        },
        {
            "name": "Library class/type",
            "scope": ["support.type", "support.class", "source.go storage.type"],
            "settings": {
                "foreground": "$color2"
            }
        },
        {
            "name": "Decorators/annotation",
            "scope": [
                "meta.decorator variable.other",
                "meta.decorator punctuation.decorator",
                "storage.type.annotation"
            ],
            "settings": {
                "foreground": "$color1"
            }
        },
        {
            "name": "Invalid",
            "scope": ["invalid"],
            "settings": {
                "foreground": "$color4"
            }
        },
        {
            "name": "diff.header",
            "scope": ["meta.diff", "meta.diff.header"],
            "settings": {
                "foreground": "$color13"
            }
        },
        {
            "name": "Ruby class methods",
            "scope": ["source.ruby variable.other.readwrite"],
            "settings": {
                "foreground": "$color11"
            }
        },
        {
            "name": "CSS tag names",
            "scope": [
                "source.css entity.name.tag",
                "source.sass entity.name.tag",
                "source.scss entity.name.tag",
                "source.less entity.name.tag",
                "source.stylus entity.name.tag"
            ],
            "settings": {
                "foreground": "$color12"
            }
        },
        {
            "name": "CSS browser prefix",
            "scope": [
                "source.css support.type",
                "source.sass support.type",
                "source.scss support.type",
                "source.less support.type",
                "source.stylus support.type"
            ],
            "settings": {
                "foreground": "${color8}b0"
            }
        },
        {
            "name": "CSS Properties",
            "scope": ["support.type.property-name"],
            "settings": {
                "fontStyle": "normal",
                "foreground": "$color2"
            }
        },
        {
            "name": "Search Results Nums",
            "scope": ["constant.numeric.line-number.find-in-files - match"],
            "settings": {
                "foreground": "${color8}b0"
            }
        },
        {
            "name": "Search Results Match Nums",
            "scope": ["constant.numeric.line-number.match"],
            "settings": {
                "foreground": "$color14"
            }
        },
        {
            "name": "Search Results Lines",
            "scope": ["entity.name.filename.find-in-files"],
            "settings": {
                "foreground": "$color10"
            }
        },
        {
            "scope": ["message.error"],
            "settings": {
                "foreground": "$color4"
            }
        },
        {
            "name": "Markup heading",
            "scope": ["markup.heading", "markup.heading entity.name"],
            "settings": {
                "fontStyle": "bold",
                "foreground": "$color10"
            }
        },
        {
            "name": "Markup links",
            "scope": ["markup.underline.link", "string.other.link"],
            "settings": {
                "foreground": "$color2"
            }
        },
        {
            "name": "Markup Italic",
            "scope": ["markup.italic"],
            "settings": {
                "fontStyle": "italic",
                "foreground": "$color1"
            }
        },
        {
            "name": "Markup Bold",
            "scope": ["markup.bold"],
            "settings": {
                "fontStyle": "bold",
                "foreground": "$color1"
            }
        },
        {
            "name": "Markup Bold/italic",
            "scope": ["markup.italic markup.bold", "markup.bold markup.italic"],
            "settings": {
                "fontStyle": "bold italic"
            }
        },
        {
            "name": "Markup Code",
            "scope": ["markup.raw"],
            "settings": {
                "background": "${foreground}06"
            }
        },
        {
            "name": "Markup Code Inline",
            "scope": ["markup.raw.inline"],
            "settings": {
                "background": "${foreground}10"
            }
        },
        {
            "name": "Markdown Separator",
            "scope": ["meta.separator"],
            "settings": {
                "fontStyle": "bold",
                "background": "${foreground}10",
                "foreground": "${color8}b0"
            }
        },
        {
            "name": "Markup Blockquote",
            "scope": ["markup.quote"],
            "settings": {
                "foreground": "$color14",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markup List Bullet",
            "scope": ["markup.list punctuation.definition.list.begin"],
            "settings": {
                "foreground": "$color11"
            }
        },
        {
            "name": "Markup added",
            "scope": ["markup.inserted"],
            "settings": {
                "foreground": "$color2"
            }
        },
        {
            "name": "Markup modified",
            "scope": ["markup.changed"],
            "settings": {
                "foreground": "$color3"
            }
        },
        {
            "name": "Markup removed",
            "scope": ["markup.deleted"],
            "settings": {
                "foreground": "$color4"
            }
        },
        {
            "name": "Markup Strike",
            "scope": ["markup.strike"],
            "settings": {
                "foreground": "$color13"
            }
        },
        {
            "name": "Markup Table",
            "scope": ["markup.table"],
            "settings": {
                "background": "${foreground}10",
                "foreground": "$color2"
            }
        },
        {
            "name": "Markup Raw Inline",
            "scope": ["text.html.markdown markup.inline.raw"],
            "settings": {
                "foreground": "$color3"
            }
        },
        {
            "name": "Markdown - Line Break",
            "scope": ["text.html.markdown meta.dummy.line-break"],
            "settings": {
                "background": "${color8}b0",
                "foreground": "${color8}b0"
            }
        },
        {
            "name": "Markdown - Raw Block Fenced",
            "scope": ["punctuation.definition.markdown"],
            "settings": {
                "background": "${foreground}",
                "foreground": "${color9}b0"
            }
        }
    ]
}
